module TypesetBase :> sig
  % `{\embed(it);}` is equivalent to `it`
  val \embed : inline [inline-text]
  val \eval : inline [(context -> inline-boxes)]
  val \eval-const : inline [inline-boxes]
  val +eval : block [(context -> block-boxes)]
  val +eval-const : block [block-boxes]
end = struct
  val inline \embed it = it
  val inline ctx \eval ibf = ibf ctx
  val inline ctx \eval-const ib = ib
  val block ctx +eval bbf = bbf ctx
  val block ctx +eval-const bb = bb
end
