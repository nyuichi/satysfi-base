use package open Math
use package open Base
use open TestUtil of `./../util`

let open Typeset in
let open Derive in
satysfi-it ${
  \proven!(
    let open DeriveDSL in
    derive ${ \Gamma \vdash \Delta }
    |> by {Cut}
    |> withLine (dotted-line 2pt 1pt)
    |> from [
      assume ${\Gamma \vdash \Delta, A},
      assume ${\Gamma, A \vdash \Delta},
    ]
  )
}
